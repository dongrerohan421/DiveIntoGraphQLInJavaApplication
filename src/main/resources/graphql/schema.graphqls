type Speaker {
    id: ID!
    name: String!
    twitter: String
}

type Attendee {
    id: ID!
    name : String!
}

type Talk {
    id: ID!
    title: String!
    description: String
}

schema {
    query: Query
}

type Query {
   allTalks: [Talk]
   allSpeakers: [Speaker]
   allAttendees: [Attendee]
}
